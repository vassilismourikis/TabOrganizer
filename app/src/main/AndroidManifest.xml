<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.taborganizer">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TabOrganizer">

        <activity
            android:name="com.example.taborganizer.DeleteList"
            android:label="Delete List"
            android:theme="@style/Theme.TabOrganizer" />
        <activity
            android:name="com.example.taborganizer.Selecter"
            android:label="My lists"
            android:theme="@style/Theme.TabOrganizer" />
        <activity
            android:name="com.example.taborganizer.Song"
            android:label="Song"
            android:theme="@style/Theme.TabOrganizer" />
        <activity
            android:name="com.example.taborganizer.ListContainer"
            android:label="Songs"
            android:theme="@style/Theme.TabOrganizer" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"  />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>